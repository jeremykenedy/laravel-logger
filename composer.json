{
    "name": "jeremykenedy/laravel-logger",
    "description": "Laravel Logger Package",
    "keywords": [
            "Laravel activity",
            "Laravel activity logger",
            "Laravel activity log",
            "Laravel logger",
            "Laravel log"
        ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "jeremykenedy",
            "email": "jeremykenedy@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.3.0|^8.0",
        "jaybizzle/laravel-crawler-detect": "1.*"
    },
    "autoload": {
        "psr-4": {
            "jeremykenedy\\LaravelLogger\\": "src/"
        },
        "files": [
            "src/App/Logic/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "jeremykenedy\\LaravelLogger\\LaravelLoggerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "require-dev": {
        "larastan/larastan": "^3.7",
        "rector/rector": "^2.2",
        "rector/rector-laravel": "^2.1"
    }
}
